                                            </li>

                                        </ul>

                                    </figure>

                                </div>
                                <div class="star">
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                </div>
                                <h4>
                                    <%= product.name %>
                                </h4>
                                <p>$<%= product.price %>
                                </p>
                                <% if (product.category=="Envío Gratis" ) { %>
                                    <span class="of2">Envío Gratis</span>
                                    <% } else if (product.category=="Nuevo" ) { %>
                                        <span class="of4">SALE</span>
                                        <% } else { %>
                                            <br>
                                            <% } %>

                            </article>

                            <% }) %>

                        </div>
                    <button type="button" class="slick-next3">
                        <i class="fas fa-chevron-right"></i>
                    </button>

                </div>
            </section> 
            <!----FIN SECCION PRODUCTOS----->

            <!----BANNER BACK PRODUCTOS----->
                <div class="banner-back-product" data-aos="fade-up" data-aos-duration="1000">
                    <img class="imagen-banner" src="/images/BANNER-BACK-PRODUCTOS.jpg" alt="" >
                </div>
               
           
                <!-------ATENCIÓN AL CLIENTE------->
                <h2 class="client">Atención Tienda Online</h2>
                <div class="fordCard" data-aos="fade-up" data-aos-duration="1000">
                    <div class="contenedor-contacto">
                        <h3><i class="far fa-envelope"></i>Mail</h3>
                        <p>Lunes a Viernes 8 a 18hs.
                            
                            Sabados de 9 a 19hs.
                            
                            Domingos 10 a 18hs.
                        
                           </p>
                    </div>
                    <div class="contenedor-contacto">
                        <h3><i class="fab fa-whatsapp"></i>Chat</h3>
                        <p>Lunes a Viernes 8 a 18hs.
                            
                            Sabados de 9 a 19hs.
                            
                            Domingos 10 a 18hs.
                        
                            </p>
                    </div>
                    <div class="contenedor-contacto">
                        <h3><i class="fas fa-headphones-alt"></i>Teléfono</h3>
                        <p>Lunes a Viernes 8 a 18hs.
                            
                            Sabados de 9 a 19hs.
                            
                            Domingos 10 a 18hs.
                        
                            </p>
                    </div>
                    
                </div>
            </main>

            <%- include('partials/footer') %>

            <%- include('partials/scripts') %>
              
    </body>

</html>