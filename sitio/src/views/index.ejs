<!DOCTYPE html>
<html lang="en">

<%- include('partials/head') %>

    <body class="header1">

        <%- include('partials/header') %>

            <main class="cont-main">

                <!-- BANNER CELU -->
                <div class="horizontal-slider" data-aos="fade-down" data-aos-easing="linear" data-aos-duration="1500">

                    <div class="slider-container">
                        <div class="item">
                            <img src="images/BANNERCEL1.jpg" alt="">
                        </div>
                        <div class="item">
                            <img src="images/BANNERCEL2.jpg" alt="">
                        </div>
                        <div class="item">
                            <img src="images/BANNERCEL3.jpg" alt="">
                        </div>
                    </div>
                </div>

                <!-- BANNER TABLET Y DESKTOP -->
                <div class="cont-padre">
                    <div class="tex-1" data-aos="fade-down" data-aos-easing="linear" data-aos-duration="1500">
                        <h1 class="h2">Renová <br> Tu Cocina</h1>
                        <p> Explora y descubrí todas nuestras ofertas <br> Válidas hasta 1/1/2022
                        </p>
                        <a href="#art" id="road">Más vendidos</a>

                        <ul class="banner-cv">
                            <li class="next active"></li>
                            <li class="next"></li>
                            <li class="next"></li>
                        </ul>
                    </div>
                    <div class="tex-2">
                        <img src="/images/BANNER1.jpg" id="banner" alt="">
                    </div>
                </div>


                <div class="imagenes-4" data-aos="fade-up" data-aos-duration="1000">
                    <img src="/images/Frame 1Bnnaeroff.jpg" alt="">
                    <img src="/images/Frame 1Bnnaeroff2.jpg" alt="">
                </div>

                <div class="titulo-sect">
                    <p class="art" data-aos="fade-right">. PRODUCTOS MAS VENDIDOS .</p>
                </div>

                <!-------SECCION PRODUCTOS------->
                <section data-aos="fade-up" data-aos-duration="1000">
                <!-------VAJILLA------->
                <div class="carousel-cont">
                    <button type="button" class="slick-prev">
                        <i class="fas fa-chevron-left"></i>
                    </button>
                   
                    <div class="caja-prac vajilla">
                        <% vajilla.forEach(product=> { %>
                            <article class="seis">
                                <div class="zomm" id="zoom">
                                    <figure>
                                        <img src="/images/products/<%=product.images[0] ? product.images[0].file : 'default-image.png'%>"
                                            alt="<%= product.name %>">
                                        <ul class="action">
                                            <li>
                                                <i class="fas fa-heart"></i>
                                                <span>Añadir a Favoritos</span>
                                            </li>
                                            <li>
                                                <i class="fas fa-shopping-cart"></i>
                                                <span>Añadir al Carrito</span>
                                            </li>
                                            <li>
                                                <a href="/products/<%= product.id %>"><i class="far fa-eye"></i></a>
                                                <a><span>Ver Detalle</span></a>
                                            </li>
                                        </ul>
                                    </figure>
                                </div>
                                <div class="star">
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                </div>
                                <h4>
                                    <%= product.name %>
                                </h4>
                                <p>$<%= product.price %>
                                </p>
                                <% if (product.category=="Envío Gratis" ) { %>
                                    <span class="of2">Envío Gratis</span>
                                    <% } else if (product.category=="Nuevo" ) { %>
                                        <span class="of4">SALE</span>
                                        <% } else { %>
                                            <br>
                                            <% } %>
                            </article>
                            <% }) %>
                        </div>
                    <button type="button" class="slick-next">
                        <i class="fas fa-chevron-right"></i>
                    </button>
                </div>


                <!-------CUBIERTOS------->
                <div class="carousel-cont">
                    <button type="button" class="slick-prev1">
                        <i class="fas fa-chevron-left"></i>
                    </button>
                    <div class="caja-prac cubiertos ">
                        <% cubiertos.forEach(product=> { %>
                            <article class="seis">
                                <div class="zomm" id="zoom">
                                    <figure>
                                        <img src="/images/products/<%=product.images[0] ? product.images[0].file : 'default-image.png'%>"
                                            alt="<%= product.name %>">
                                        <ul class="action">
                                            <li>
                                                <i class="fas fa-heart"></i>
                                                <span>Añadir a Favoritos</span>
                                            </li>
                                            <li>
                                                <i class="fas fa-shopping-cart"></i>
                                                <span>Añadir al Carrito</span>
                                            </li>
                                            <li>
                                                <a href="/products/<%= product.id %>"><i class="far fa-eye"></i></a>
                                                <a><span>Ver Detalle</span></a>
                                            </li>

                                        </ul>

                                    </figure>

                                </div>
                                <div class="star">
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></ <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                </div>
                                <h4>
                                    <%= product.name %>
                                </h4>
                                <p>$<%= product.price %>
                                </p>
                                <% if (product.category=="Envío Gratis" ) { %>
                                    <span class="of2">Envío Gratis</span>
                                    <% } else if (product.category=="Nuevo" ) { %>
                                        <span class="of4">SALE</span>
                                        <% } else { %>
                                            <br>
                                            <% } %>

                            </article>

                            <% }) %>
                        </div>
                    <button type="button" class="slick-next1">
                        <i class="fas fa-chevron-right"></i>
                    </button>
                </div>


                <!-------CAFE Y TE------->
                <div class="carousel-cont">
                    <button type="button" class="slick-prev2">
                        <i class="fas fa-chevron-left"></i>
                    </button>
                    <div class="caja-prac tazas">
                        <% tazas.forEach(product=> { %>

                            <article class="seis">

                                <div class="zomm" id="zoom">
                                    <figure>
                                        <img src="/images/products/<%=product.images[0] ? product.images[0].file : 'default-image.png'%>"
                                            alt="<%= product.name %>">
                                        <ul class="action">
                                            <li>
                                                <i class="fas fa-heart"></i>
                                                <span>Añadir a Favoritos</span>
                                            </li>
                                            <li>
                                                <i class="fas fa-shopping-cart"></i>
                                                <span>Añadir al Carrito</span>
                                            </li>
                                            <li>
                                                <a href="/products/<%= product.id %>"><i class="far fa-eye"></i></a>
                                                <a><span>Ver Detalle</span></a>
                                            </li>
                                        </ul>

                                    </figure>

                                </div>
                                <div class="star">
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                    <i class="fas fa-star"></i>
                                </div>
                                <h4>
                                    <%= product.name %>
                                </h4>
                                <p>$<%= product.price %>
                                </p>
                                <% if (product.category=="Envío Gratis" ) { %>
                                    <span class="of2">Envío Gratis</span>
                                    <% } else if (product.category=="Nuevo" ) { %>
                                        <span class="of4">SALE</span>
                                        <% } else { %>
                                            <br>
                                            <% } %>

                            </article>

                            <% }) %>

                        </div>
                    <button type="button" class="slick-next2">
                        <i class="fas fa-chevron-right"></i>
                    </button>
                </div>

                <!-------CRISTALERIA------->
                <div class="carousel-cont">
                    <button type="button" class="slick-prev3">
                        <i class="fas fa-chevron-left"></i>
                    </button>
                    <div class="caja-prac cristaleria">
                        <% cristaleria.forEach(product=> { %>

                            <article class="seis">

                                <div class="zomm" id="zoom">
                                    <figure>
                                        <img src="/images/products/<%=product.images[0] ? product.images[0].file : 'default-image.png'%>"
                                            alt="<%= product.name %>">
                                        <ul class="action">
                                            <li>
                                                <i class="fas fa-heart"></i>
                                                <span>Añadir a Favoritos</span>
                                            </li>
                                            <li>
                                                <i class="fas fa-shopping-cart"></i>
                                                <span>Añadir al Carrito</span>
                                            </li>
                                            <li>
                                                <a href="/products/<%= product.id %>"><i class="far fa-eye"></i></a>
